<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SellerpainterActivity">

    <GridLayout
        android:id="@+id/gridLayout"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <EditText
            android:id="@+id/textpainterdescription6"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_row="6"
            android:layout_column="1"
            android:hint="Enter Your Description"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/painter_description10"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_row="6"
            android:layout_column="0"
            android:fontFamily="serif"
            android:text="Descrpition"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textSize="14sp" />

        <EditText
            android:id="@+id/textpainterdescription2"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_row="2"
            android:layout_column="1"
            android:hint="Enter Your Service Type"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/textpainterdescription3"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_row="5"
            android:layout_column="1"
            android:hint="Enter Your Price"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/textpainterdescription5"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_row="4"
            android:layout_column="1"
            android:hint="Enter Your Experience"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/painter_description3"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_row="2"
            android:layout_column="0"
            android:fontFamily="serif"
            android:text="Service Type"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/painter_description4"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_row="5"
            android:layout_column="0"
            android:fontFamily="serif"
            android:text="Price"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/painter_description9"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_row="3"
            android:layout_column="0"
            android:fontFamily="serif"
            android:text="Expertise"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textSize="14sp" />

        <TextView
            android:id="@+id/painter_phoneno"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_row="1"
            android:layout_column="0"
            android:fontFamily="serif"
            android:text="Phone Number"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textSize="14sp" />

        <EditText
            android:id="@+id/textpaintername"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_row="0"
            android:layout_column="1"
            android:hint="Enter your Name"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textStyle="bold" />

        <EditText
            android:id="@+id/textpainterphoneno"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_row="1"
            android:layout_column="1"
            android:hint="Enter Your Phone No"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/painter_description"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_row="4"
            android:layout_column="0"
            android:fontFamily="serif"
            android:text="Experience"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textSize="14sp" />

        <EditText
            android:id="@+id/textpainterdescription"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_row="3"
            android:layout_column="1"
            android:hint="Enter Your Expertise"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/painter_name"
            android:layout_width="150dp"
            android:layout_height="35dp"
            android:layout_row="0"
            android:layout_column="0"
            android:fontFamily="serif"
            android:text="Name"
            android:textAlignment="textStart"
            android:textAppearance="@style/TextAppearance.AppCompat.Body1"
            android:textSize="14sp"
            app:layout_column="1"
            app:layout_row="1" />


    </GridLayout>

    <Button
        android:id="@+id/sellerpainterbutton"
        android:layout_width="132dp"
        android:layout_height="48dp"
        android:layout_marginTop="76dp"
        android:background="@drawable/custom_button"
        android:text="ADD"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.49"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gridLayout"
        app:layout_constraintVertical_bias="0.0" />


</androidx.constraintlayout.widget.ConstraintLayout>